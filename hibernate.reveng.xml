<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="SIMP" match-name="BOUNDLOG"/>
  <table-filter match-schema="SIMP" match-name="CONFIG"/>
  <table-filter match-schema="SIMP" match-name="SIMP_APPLICATION"/>
  <table-filter match-schema="SIMP" match-name="SIMP_BOUNDAPPINFO"/>
  <table-filter match-schema="SIMP" match-name="SIMP_BOUNDUSERINFO"/>
  <table-filter match-schema="SIMP" match-name="SIMP_DEVICEINFO"/>
  <table-filter match-schema="SIMP" match-name="VERSIONLOG"/>
</hibernate-reverse-engineering>